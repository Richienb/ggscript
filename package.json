{
    "name": "ggscript",
    "description": "Inject scripts into any website.",
    "keywords": [
        "script",
        "inject",
        "href",
        "website"
    ],
    "version": "0.0.0",
    "main": "dist/index.js",
    "repository": "https://github.com/Richienb/ggscript.git",
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build:js": "tsc",
        "build:dist": "rollup -c",
        "build:min": "terser --compress --mangle -o dist/index.min.js --source-map \"filename='index.min.js.map',root='index.js.map',url='index.min.js.map'\" -- dist/index.js",
        "build:docs": "typedoc --out ./docs --mode file ./src",
        "build": "yarn build:js && yarn build:dist && yarn build:min && yarn build:docs",
        "lint": "eslint src/index.ts"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^1.7.0",
        "eslint": "^5.16.0",
        "eslint-config-google": "^0.12.0",
        "rollup": "^1.10.1",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-resolve": "^4.2.3",
        "rollup-plugin-terser": "^4.0.4",
        "rollup-plugin-typescript": "^1.0.1",
        "terser": "^4.0.2",
        "tslib": "^1.9.3",
        "typedoc": "^0.14.2",
        "typescript": "^3.4.5"
    },
    "dependencies": {
        "@types/bluebird": "^3.5.26",
        "axios": "^0.18.0",
        "bluebird": "^3.5.4"
    }
}
